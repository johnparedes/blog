<%= form_for @entry do |f| %>
  <p><div class="field">
    <%= f.label :title %><br/>
    <%= f.text_field :title %>
  </div></p>
  <p><div class="field">
    <%= f.label :post %><br/>
    <%= f.text_area :post %>
  </div></p>
  <p><div class="field">
    <%= f.label :date_posted %><br/>
    <%= f.date_field :date_posted %>
  </div></p>
  <%= f.submit %>
<% end %>
